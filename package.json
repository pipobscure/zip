{
	"name": "@pipobscure/zip",
	"version": "2.0.2",
	"description": "Compressed Zip Archive",
	"keywords": [
		"Zip",
		"Archive",
		"Compressed"
	],
	"license": "EUPL-1.2",
	"author": "Philipp Dunkel <pip@pipobscure.com>",
	"type": "module",
	"exports": {
		".": "./src/zip.js",
		"./file": "./src/file.js",
		"./buffer": "./src/buffer.js"
	},
	"files": [
		"src/zip.js",
		"src/zip.d.ts",
		"src/file.js",
		"src/file.d.ts",
		"src/buffer.js",
		"src/buffer.d.ts"
	],
	"scripts": {
